request.bundle = type=http-request, pattern=^https?:\/\/(www|m|tv)\.youtube\.com\/youtubei\/v1\/player(\?.+)?$, script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/ddgksf2013/DualSubs.YouTube/DualSubs_request.bundle.js, requires-body=true, binary-body-mode=true, max-size=-1, timeout=60

# üçøÔ∏è DualSubs.YouTube.Player.response.json
response.bundle = type=http-response, pattern=^https?:\/\/(www|m|tv)\.youtube\.com\/youtubei\/v1\/player(\?.+)?$, script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/ddgksf2013/DualSubs.YouTube/DualSubs_response.bundle.js, requires-body=true, binary-body-mode=true, max-size=-1, timeout=60

# üçøÔ∏è DualSubs.YouTube.Player.request.proto
