[Script]
Dualsub = type=http-response, pattern=^http.+(media.(dss|star)ott|manifests.v2.api.hbo|hbomaxcdn|nflxvideo|cbs(aa|i)video|cloudfront|akamaihd|avi-cdn|huluim|youtube).(com|net)\/(.+\.vtt($|\?m=\d+)|.+-all-.+\.m3u8.*|hls\.m3u8.+|\?o=\d+&v=\d+&e=.+|\w+\/2\$.+\/[a-zA-Z0-9-]+\.m3u8|api\/timedtext.+), script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/ddgksf2013/Dualsub/Dualsub.js, requires-body=true, max-size=-1, timeout=60

Dualsub = type=http-request, pattern=^http.+(setting|general).(media.dssott|hbomaxcdn|nflxvideo|youtube|cbsivideo|cloudfront|huluim).(com|net)\/\?action=(g|s)et, script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/ddgksf2013/Dualsub/Dualsub.js, requires-body=true, max-size=-1, timeout=60

[MITM]
hostname = %APPEND% *.media.dssott.com, *.media.starott.com, *.api.hbo.com, *.hbomaxcdn.com, *.huluim.com, *.nflxvideo.net, *.cbsaavideo.com, *.cbsivideo.com, *.cloudfront.net, *.akamaihd.net, *.avi-cdn.net, *.youtube.com

