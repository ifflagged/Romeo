[Rule]
DOMAIN,ifconfig.me,REJECT,extended-matching,pre-matching

[Map Local]
^https?:\/\/mapi\.chaogejiaoyu\.com\/api\/ad\/adCheck data-type=text data="{}" status-code=200

[Script]
chaoge = type=http-response, pattern=^https?:\/\/mapi\.chaogejiaoyu\.com\/api\/ad\/getPageAdList, script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/zirawell/chaoge/chaoge.js, requires-body=true, max-size=0

chaoge = type=http-response, pattern=^https?:\/\/mapi\.chaogejiaoyu\.com\/api\/outline\/getAppBanner, script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/zirawell/chaoge/chaoge.js, requires-body=true, max-size=0

[MITM]
hostname = %APPEND% mapi.chaogejiaoyu.com

