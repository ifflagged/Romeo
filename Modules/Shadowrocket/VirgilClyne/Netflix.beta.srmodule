[Script]
🍟 Netflix Web = type=http-response, pattern=^https?:\/\/www\.netflix\.com\/(api\/ftl\/probe), script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Netflix.beta.js, requires-body=true, max-size=0

🍟 Netflix iOS = type=http-response, pattern=^https?:\/\/(ios|ios-anycast|ios-prefix)\.prod\.(ftl|cloud|http1)\.netflix\.com\/, script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Netflix.beta.js, requires-body=true, max-size=0

🍟 Netflix Android = type=http-response, pattern=^https?:\/\/(android|android-anycast|android-prefix)\.prod\.(ftl|cloud)\.netflix\.com\/, script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Netflix.beta.js, requires-body=true, max-size=0

🍟 Netflix Chromecast = type=http-response, pattern=^https?:\/\/(cast|cast-uiboot|android-prefix)\.prod\.(http1)\.netflix\.com\/, script-path=https://raw.githubusercontent.com/VirgilClyne/GetSomeFries/beta/js/Netflix.beta.js, requires-body=true, max-size=0

[MITM]
hostname = %APPEND% www.netflix.com, *.prod.ftl.netflix.com, *.prod.cloud.netflix.com

