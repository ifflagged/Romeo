[Rule]
DOMAIN,known-issues.apple.com,REJECT

[URL Rewrite]
^https?:\/\/play-cdn\.itunes\.apple\.com https://play.itunes.apple.com header
^https?:\/\/play-edge-cdn\.itunes\.apple\.com https://play-edge.itunes.apple.com header

[Script]
📺 TV.uts.request = type=http-request, pattern=^https?:\/\/uts-api\.itunes\.apple\.com\/uts\/(v1|v2|v3)\/, requires-body=1, script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/NSRingo/iRingo.TV/request.bundle.js, argument=
📺 TV.uts.response = type=http-response, pattern=^https?:\/\/uts-api\.itunes\.apple\.com\/uts\/(v1|v2|v3)\/, requires-body=1, script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/NSRingo/iRingo.TV/response.bundle.js, argument=
📺 TV.umc.request = type=http-request, pattern=^https?:\/\/umc-tempo-api\.apple\.com\/(v1|v2|v3)\/, requires-body=1, script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/NSRingo/iRingo.TV/request.bundle.js, argument=

[MITM]
hostname = %APPEND% uts-api.itunes.apple.com, umc-tempo-api.apple.com, play-cdn.itunes.apple.com, play-edge-cdn.itunes.apple.com
