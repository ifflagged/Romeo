[Header Rewrite]
# 解决GitHub速率限制返回429问题
http-request ^https:\/\/.*\.githubusercontent\.com\/ header-replace 'Accept-Language' 'en-us'
http-request ^https:\/\/github\.com\/ header-replace 'Accept-Language' 'en-us'

[MITM]
hostname = %APPEND% github.com, *.githubusercontent.com

