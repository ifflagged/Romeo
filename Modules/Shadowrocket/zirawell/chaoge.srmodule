[Rule]
DOMAIN,ifconfig.me,REJECT,extended-matching

[Map Local]
^https?:\/\/mapi\.chaogejiaoyu\.com\/api\/ad\/adCheck data-type=text data="{}"

[Script]
chaoge = type=http-response, pattern=^https?:\/\/mapi\.chaogejiaoyu\.com\/api\/ad\/getPageAdList, script-path=https://raw.githubusercontent.com/zirawell/R-Store/main/Res/Scripts/AntiAd/chaoge.js, requires-body=true, max-size=0

chaoge = type=http-response, pattern=^https?:\/\/mapi\.chaogejiaoyu\.com\/api\/outline\/getAppBanner, script-path=https://raw.githubusercontent.com/zirawell/R-Store/main/Res/Scripts/AntiAd/chaoge.js, requires-body=true, max-size=0

[MITM]
hostname = %APPEND% mapi.chaogejiaoyu.com

