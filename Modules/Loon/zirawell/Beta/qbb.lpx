^https?:\/\/apipt\.qbb6\.com\/parenting\/pt\/home\/post\/card\/get mock-response-body data-type=text data="{}" status-code=200
^https?:\/\/apicommunity\.qbb6\.com\/community\/category\/list\/get mock-response-body data-type=text data="{}" status-code=200

[Script]
