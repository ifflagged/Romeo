[General]
force-http-engine-hosts = script.hub, *.script.hub

[Rule]
AND,((DOMAIN-SUFFIX,ddgksf2013.top),(PROTOCOL,QUIC)),REJECT-NO-DROP

[Script]
http-response ^https?:\/\/ddgksf2013.top\/$ script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/ddgksf2013/ScriptHub.Egern/ddgksf2013.top_web.egern.js, requires-body=true, timeout=3500, tag=网页变身

http-request ^https?:\/\/script\.hub\/file\/_start_\/.+type=(?:qx-rewrite|surge-module|all-module) script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/ddgksf2013/ScriptHub.Egern/Script-Hub-Org_Rewrite-Parser.js, timeout=300, tag=脚本转换

[MITM]
hostname = script.hub, *.script.hub, ddgksf2013.top

