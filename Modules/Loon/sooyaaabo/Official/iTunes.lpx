[Argument]
iTunes-chxm1023 = switch, false, true, tag = iTunes-chxm1023, desc = 是否启用chxm1023的脚本来解锁iTunes系列
iTunes-ddgksf2013 = switch, false, true, tag = iTunes-ddgksf2013, desc = 是否启用ddgksf2013的脚本来解锁iTunes系列
iTunes-Guding88 = switch, false, true, tag = iTunes-Guding88, desc = 是否启用Guding88的脚本来解锁iTunes系列

[Script]
# chxm1023
http-response ^https?:\/\/buy\.itunes\.apple\.com\/verifyReceipt$ script-path = https://github.com/ifflagged/Romeo/raw/main/Modules/JS/sooyaaabo/iTunes/iTunes.js, requires-body = true, tag = [iTunes]解锁会员@chxm1023, enable={iTunes-chxm1023}

# ddgksf2013
http-response ^https?:\/\/buy\.itunes\.apple\.com\/verifyReceipt$ script-path = https://github.com/ifflagged/Romeo/raw/main/Modules/JS/sooyaaabo/iTunes/iTunes-ddgksf2013.js, requires-body = true, tag = [iTunes]解锁会员@ddgksf2013, enable={iTunes-ddgksf2013}

# Guding88
http-response ^https?:\/\/buy\.itunes\.apple\.com\/verifyReceipt$ script-path = https://github.com/ifflagged/Romeo/raw/main/Modules/JS/sooyaaabo/iTunes/APPheji_iTunes.js, requires-body = true, tag = [iTunes]解锁会员@Guding88, enable={iTunes-Guding88}

[MitM]
hostname = buy.itunes.apple.com