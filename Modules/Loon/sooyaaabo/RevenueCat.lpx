#!name=RevenueCat
#!desc=RevenueCat系列解锁合集，在设置中选择需要使用的原作者脚本以使用此插件。
#!author=chxm1023[https://github.com/chxm1023], ddgksf2013[https://github.com/ddgksf2013], Guding88[https://github.com/Guding88], Yu9191[https://github.com/Yu9191]
#!icon=icon not found
#!tag=Jacob
#!tag=解锁会员
#!loon_version=3.3.7(921)
#!homepage=https://loonlab.103516.xyz/
#!date=2026-01-07 20:00

[Argument]
RevenueCat-chxm1023=switch,"false","true",tag = RevenueCat-chxm1023, desc = 是否启用chxm1023的脚本来解锁RevenueCat系列
RevenueCat-Guding88=switch,"false","true",tag = RevenueCat-Guding88, desc = 是否启用Guding88的脚本来解锁RevenueCat系列
RevenueCat-ddgksf2013=switch,"false","true",tag = RevenueCat-ddgksf2013, desc = 是否启用ddgksf2013的脚本来解锁RevenueCat系列
RevenueCat-Yu9191=switch,"false","true",tag = RevenueCat-Yu9191, desc = 是否启用Yu9191的脚本来解锁RevenueCat系列

[Rewrite]
^https:\/\/api\.lianjiu\.fun\/app\/api\/v1\/profile - reject

[Script]
# chxm1023
http-response ^https:\/\/api\.(revenuecat|rc-backup)\.com\/.+\/(receipts$|subscribers\/?(.*?)*$) script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/sooyaaabo/RevenueCat/chxm1023_Reheji.js, requires-body=true, tag=[RevenueCat]会员解锁响应@chxm1023, enable={RevenueCat-chxm1023}

http-request ^https:\/\/api\.(revenuecat|rc-backup)\.com\/.+\/(receipts$|subscribers\/?(.*?)*$) script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/sooyaaabo/RevenueCat/chxm1023_Reheji.js, tag=[RevenueCat]会员解锁请求@chxm1023, enable={RevenueCat-chxm1023}

# Guding88
http-response ^https?:\/\/api\.(revenuecat|rc-backup)\.com\/v1\/(subscribers\/[^\/]+$|receipts$) script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/sooyaaabo/RevenueCat/Guding88_APPheji_Revenuecat.js, requires-body=true, tag=[RevenueCat]会员解锁响应@Guding88, enable={RevenueCat-Guding88}

http-request ^https?:\/\/api\.(revenuecat|rc-backup)\.com\/v1\/(subscribers\/[^\/]+$|receipts$) script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/sooyaaabo/RevenueCat/Guding88_APPheji_Revenuecat.js, tag=[RevenueCat]会员解锁请求@Guding88, enable={RevenueCat-Guding88}

# ddgksf2013
http-response ^https:\/\/api\.(revenuecat|rc-backup)\.com\/.+\/(receipts$|subscribers\/?(.*?)*$) script-path=https://ddgksf2013.top/scripts/revenuecat.vip.js, requires-body=true, tag=[RevenueCat]会员解锁响应@ddgksf2013, enable={RevenueCat-ddgksf2013}

http-request ^https:\/\/api\.(revenuecat|rc-backup)\.com\/.+\/(receipts$|subscribers\/?(.*?)*$) script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/sooyaaabo/RevenueCat/ddgksf2013_deleteHeader.js, tag=[RevenueCat]会员解锁请求@ddgksf2013, enable={RevenueCat-ddgksf2013}

# Yu9191
http-response ^https:\/\/(api\.revenuecat\.com|api\.rc-backup\.com)\/.+\/(receipts$|subscribers\/[^/]+$) script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/sooyaaabo/RevenueCat/Yu9191_Revenuecat.js, requires-body=true, tag=[RevenueCat]会员解锁响应@Yu9191, enable={RevenueCat-Yu9191}

http-request ^https:\/\/(api\.revenuecat\.com|api\.rc-backup\.com)\/.+\/(receipts$|subscribers\/[^/]+$) script-path=https://github.com/ifflagged/Romeo/raw/main/Modules/JS/sooyaaabo/RevenueCat/Yu9191_Revenuecat.js, tag=[RevenueCat]会员解锁请求@Yu9191, enable={RevenueCat-Yu9191}

[MITM]
hostname = api.revenuecat.com, api.rc-backup.com, api.lianjiu.fun

