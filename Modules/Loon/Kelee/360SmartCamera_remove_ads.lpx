[Rule]
DOMAIN,ad.jia.360.cn,REJECT
DOMAIN,ad.iot.360.cn,REJECT

[Script]
http-response ^https:\/\/jia\.360\.cn\/conf\/v1\.json script-path=https://raw.githubusercontent.com/Script-Hub-Org/Script-Hub/main/scripts/body-rewrite.js, requires-body=true, timeout=30, tag=body_rewrite_15, argument=%5B%5B%22json-del%22%2C%5B%22tab_conf%22%2C%22ad_conf%22%2C%22activity%22%5D%5D%5D

[MITM]
hostname = jia.360.cn

